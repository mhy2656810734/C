#include<stdio.h>
int main()
{
    int n=0;
    int m=0;
    int arr1[10][10];
    int arr2[10][10];
    scanf("%d %d",&n,&m);
    int i=0;
    int j=0;
    int count=1;//记录两个数组是否相等
    //分别输入两个二维数组的元素
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            scanf("%d",&arr1[i][j]);
        }
    }
       for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            scanf("%d",&arr2[i][j]);
        }
    }
    //如果有个元素相等就输出no
       for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            if(arr1[i][j]!=arr2[i][j])
            {
                 printf("No\n");
                 count=0;
                  break;
            }
        }
           if(count==0)
               break;
    }
    if(count==1)
    {
        printf("Yes\n");
    }
    return 0;
}
