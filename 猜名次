
/* 5位运动员参加了10米台跳水比赛，有人让他们预测比赛结果：
    A选手： B第二，我第三
    B选手:  我第二，E第四
    C选手： 我第一，D第二
    D选手： C最后，我第三
    E选手： 我第四，A第一
    比赛结束后，每位选手都说对了一半
*/
#include<stdio.h>
int main()
{
    int a = 0, b = 0, c = 0, d = 0, e = 0;//5位选手
    for (a = 1; a <= 5; a++)
    {
        for (b = 1; b <= 5; b++)
        {
            for (c = 1; c <= 5; c++)
            {
                for (d = 1; d <= 5; d++)
                {
                    for (e = 1; e <= 5; e++)
                    {
                        //根据每人说对一半开始判定
                        if (((b == 2) + (a == 3) == 1) &&
                            ((b == 2) + (e == 4) == 1) &&
                            ((c == 1) + (d == 2) == 1) &&
                            ((c == 5) + (d == 3) == 1) &&
                            ((e == 4) + (a == 1) == 1))
                        {
                            if (a * b * c * d * e==120)//名次是12345
                                printf("a是第%d名  b是第%d名  c是第%d名  d是第%d名  e是第%d名  \n",a,b,c,d,e);
                        }
                    }
                }
            }
        }
    }
    return 0;
}
